<!-- Custom layout file for XGrammar Doxygen documentation -->
<doxygenlayout version="1.0">
  <!-- Navigation index -->
  <navindex>
    <tab type="mainpage" visible="yes" title="Home"/>
    <tab type="pages" visible="yes" title="Related Pages"/>
    <tab type="modules" visible="yes" title="Modules"/>
    <tab type="namespaces" visible="yes" title="Namespaces">
      <tab type="namespacelist" visible="yes" title="Namespace List"/>
      <tab type="namespacemembers" visible="yes" title="Namespace Members"/>
    </tab>
    <tab type="classes" visible="yes" title="Classes">
      <tab type="classlist" visible="yes" title="Class List"/>
      <tab type="classindex" visible="yes" title="Class Index"/>
      <tab type="hierarchy" visible="yes" title="Class Hierarchy"/>
      <tab type="classmembers" visible="yes" title="Class Members"/>
    </tab>
    <tab type="files" visible="yes" title="Files">
      <tab type="filelist" visible="yes" title="File List"/>
      <tab type="globals" visible="yes" title="File Members"/>
    </tab>
    <tab type="examples" visible="yes" title="Examples"/>
  </navindex>

  <!-- Layout for a file page -->
  <file>
    <briefdescription visible="yes"/>
    <includes visible="$SHOW_INCLUDE_FILES"/>
    <includegraph visible="$INCLUDE_GRAPH"/>
    <includedbygraph visible="$INCLUDED_BY_GRAPH"/>
    <sourcelink visible="yes"/>
    <memberdecl>
      <classes visible="yes" title="Classes"/>
      <namespaces visible="yes" title="Namespaces"/>
      <constantgroups visible="yes" title="Modules"/>
      <defines title="Macros"/>
      <typedefs title="Typedefs"/>
      <sequences title="Sequences"/>
      <dictionaries title="Dictionaries"/>
      <enums title="Enumerations"/>
      <functions title="Functions"/>
      <variables title="Variables"/>
    </memberdecl>
    <detaileddescription title="Detailed Description"/>
    <memberdef>
      <inlineclasses title=""/>
      <defines title="Macro Definition Documentation"/>
      <typedefs title="Typedef Documentation"/>
      <sequences title="Sequence Documentation"/>
      <dictionaries title="Dictionary Documentation"/>
      <enums title="Enumeration Type Documentation"/>
      <functions title="Function Documentation"/>
      <variables title="Variable Documentation"/>
    </memberdef>
  </file>

